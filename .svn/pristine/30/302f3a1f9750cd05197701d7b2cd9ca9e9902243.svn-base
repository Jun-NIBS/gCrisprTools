

load.data <- function(storedir) 
{
	cds = readRDS(paste0(storedir,"cds.rds"))
	acCds = readRDS(paste0(storedir,"acCds.rds"))
	se = readRDS(paste0(storedir,"se.rds"))

	esetNorm = readRDS(paste0(storedir,"esetNorm.rds"))
	esetFilt = readRDS(paste0(storedir,"esetFilt.rds"))
	esetCounts = readRDS(paste0(storedir,"esetCounts.rds"))
	esetFiltNorm = readRDS(paste0(storedir,"esetFiltNorm.rds"))
	list(cds=cds, acCds=acCds, se=se, esetNorm = esetNorm, esetFilt = esetFilt, esetCounts=esetCounts, esetFiltNorm=esetFiltNorm)
}
